<?xml version="1.0"?>
<!DOCTYPE Code::Blocks_project_file>
<Code::Blocks_project_file>
	<FileVersion major="1" minor="1"/>
	<Project>
		<Option title="MeshUpgrader"/>
		<Option makefile="Makefile"/>
		<Option makefile_is_custom="0"/>
		<Option compiler="0"/>
		<Build>
			<Target title="Debug">
				<Option output="..\bin\debug\ogremeshupgrade.exe"/>
				<Option object_output="..\obj\debug"/>
				<Option deps_output=".deps"/>
				<Option external_deps=""/>
				<Option type="1"/>
				<Option compiler="0"/>
				<Option includeInTargetAll="0"/>
				<Compiler>
					<Add option="-DTIXML_USE_STL"/>
					<Add option="-W"/>
					<Add option="-g"/>
					<Add directory="..\include"/>
					<Add directory="..\..\..\OgreMain\include"/>
					<Add directory="..\..\..\Dependencies\include"/>
				</Compiler>
				<Linker>
					<Add library="OgreMain_d"/>
					<Add directory="..\..\..\OgreMain\lib\Debug"/>
					<Add directory="..\..\..\Dependencies\lib\Debug"/>
				</Linker>
				<ExtraCommands>
					<Add after="copy ..\bin\Debug\OgreMeshUpgrade.exe ..\..\Common\bin\Debug"/>
				</ExtraCommands>
			</Target>
			<Target title="Release">
				<Option output="..\bin\Release\OgreMeshUpgrade.exe"/>
				<Option object_output="..\obj\Release"/>
				<Option deps_output="C:\Downloads\Ogre\build\ogrenew\Tools\MeshUpgrader\scripts\.deps"/>
				<Option external_deps=""/>
				<Option type="1"/>
				<Option compiler="0"/>
				<Compiler>
					<Add option="-W"/>
					<Add option="-g"/>
					<Add option="-DTIXML_USE_STL"/>
					<Add directory="..\include"/>
					<Add directory="..\..\..\OgreMain\include"/>
					<Add directory="..\..\..\Dependencies\include"/>
				</Compiler>
				<Linker>
					<Add library="OgreMain"/>
					<Add directory="..\..\..\OgreMain\lib\Release"/>
					<Add directory="..\..\..\Dependencies\lib\Release"/>
				</Linker>
				<ExtraCommands>
					<Add after="..\..\..\Dependencies\bin\cp.exe ..\bin\Release\OgreMeshUpgrade.exe ..\..\Common\bin\Release"/>
					<Add after="strip ..\..\Common\bin\Release\OgreMeshUpgrade.exe"/>
				</ExtraCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-g"/>
		</Compiler>
		<Linker>
			<Add option="-Wl,--enable-runtime-pseudo-reloc"/>
		</Linker>
		<Unit filename="..\src\main.cpp">
			<Option compilerVar="CPP"/>
			<Option target="Debug"/>
			<Option target="Release"/>
		</Unit>
	</Project>
</Code::Blocks_project_file>
